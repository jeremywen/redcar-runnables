{
    "commands": [
        {
            "name":         "clean",
            "command":      "grails clean",
            "description":  "Clean grails web project",
            "type":         "grails/app"
        },
        {
            "name":         "run application",
            "command":      "grails run-app",
            "description":  "Start grails web project",
            "type":         "grails/app"
        },
        {
            "name":         "documentation",
            "command":      "grails doc",
            "description":  "Generate app documentation",
            "type":         "grails/app"
        },
        {
            "name":         "run war",
            "command":      "grails run-war",
            "description":  "Start grails web project from war",
            "type":         "grails/app"
        },
        {
            "name":         "dependency report",
            "command":      "grails dependency-report",
            "description":  "TRun ivy dependency report",
            "type":         "grails/report"
        },
        {
            "name":         "run all tests",
            "command":      "grails test-app -clean",
            "description":  "Test grails web project",
            "type":         "grails/test"
        },
        {
            "name":         "run integration tests",
            "command":      "grails test-app -integration -clean",
            "description":  "Run environment integration tests",
            "type":         "grails/test"
        },
        {
            "name":         "run unit tests",
            "command":      "grails test-app -unit -clean",
            "description":  "Run JUnit tests",
            "type":         "grails/test"
        },
        {
            "name":         "run features",
            "command":      "grails cucumber run-app",
            "description":  "Run cucumber features (requires features plugin)",
            "type":         "grails/test"
        },
        {
            "name":         "create war",
            "command":      "grails war",
            "description":  "Create deployable war",
            "type":         "grails/app"
        },
        {
            "name":         "create app",
            "command":      "grails create-app __PARAMS__",
            "description":  "Create a new grails application",
            "type":         "grails/create"
        },
        {
            "name":         "create controller",
            "command":      "grails create-controller __PARAMS__",
            "description":  "Create a new controller",
            "type":         "grails/create"
        },
        {
            "name":         "create domain class",
            "command":      "grails create-domain-class __PARAMS__",
            "description":  "Create a new domain class",
            "type":         "grails/create"
        },
        {
            "name":         "create filters",
            "command":      "grails create-filters __PARAMS__",
            "description":  "Create new filters",
            "type":         "grails/create"
        },
        {
            "name":         "create integration test",
            "command":      "grails create-integration-test __PARAMS__",
            "description":  "Create a new integration test",
            "type":         "grails/create"
        },
        {
            "name":         "create plugin",
            "command":      "grails create-plugin __PARAMS__",
            "description":  "Create a new plugin",
            "type":         "grails/plugins"
        },
        {
            "name":         "create script",
            "command":      "grails create-script __PARAMS__",
            "description":  "Create a new script",
            "type":         "grails/create"
        },
        {
            "name":         "create service",
            "command":      "grails create-service __PARAMS__",
            "description":  "Create a new grails service",
            "type":         "grails/create"
        },
        {
            "name":         "create tag library",
            "command":      "grails create-tag-lib __PARAMS__",
            "description":  "Create a new grails tag library",
            "type":         "grails/create"
        },
        {
            "name":         "create unit test",
            "command":      "grails create-unit-test __PARAMS__",
            "description":  "Create a new unit test",
            "type":         "grails/create"
        },
        {
            "name":         "generate all",
            "command":      "grails generate-all __PARAMS__",
            "description":  "Generate a controller and views for a domain",
            "type":         "grails/create"
        },
        {
            "name":         "generate controller",
            "command":      "grails generate-controller __PARAMS__",
            "description":  "Generate a new controller for a domain",
            "type":         "grails/create"
        },
        {
            "name":         "generate views",
            "command":      "grails generate-views __PARAMS__",
            "description":  "Generate view for a domain",
            "type":         "grails/create"
        },
        {
            "name":         "help",
            "command":      "grails help",
            "description":  "Help with grails commands",
            "type":         "grails"
        },
        {
            "name":         "init",
            "command":      "grails __PARAMS__ init",
            "description":  "Initialize",
            "type":         "grails/app"
        },
        {
            "name":         "install dependency",
            "command":      "grails install-dependency __PARAMS__",
            "description":  "Install a new dependency",
            "type":         "grails/install"
        },
        {
            "name":         "install plugin",
            "command":      "grails install-plugin __PARAMS__",
            "description":  "Install a new plugin",
            "type":         "grails/plugins"
        },
        {
            "name":         "install templates",
            "command":      "grails install-templates __PARAMS__",
            "description":  "Install new templates",
            "type":         "grails/install"
        },
        {
            "name":         "list plugins",
            "command":      "grails list-plugins",
            "description":  "List available plugins",
            "type":         "grails/plugins"
        },
        {
            "name":         "package plugins",
            "command":      "grails package-plugin",
            "description":  "Package plugin for distribution",
            "type":         "grails/plugins"
        },
        {
            "name":         "plugin info",
            "command":      "grails plugin-info __PARAMS__",
            "description":  "View information about a plugin",
            "type":         "grails/plugins"
        },
        {
            "name":         "release plugin",
            "command":      "grails release-plugin",
            "description":  "View information about a plugin",
            "type":         "grails/plugins"
        },
        {
            "name":         "set version",
            "command":      "grails set-version __PARAMS__",
            "description":  "Set the app's version in its metadata",
            "type":         "grails/app"
        },
        {
            "name":         "stats",
            "command":      "grails stats",
            "description":  "View basic information about the grails app",
            "type":         "grails/app"
        },
        {
            "name":         "uninstall plugin",
            "command":      "grails uninstall-plugin __PARAMS__",
            "description":  "Uninstall a plugin",
            "type":         "grails/plugins"
        }
    ],
    "file_runners":[
        {
            "regex":        ".*.groovy",
            "name":         "Run as groovy",
            "command":      "groovy __PATH__",
            "type":         "script/groovy"
        },
        {
            "regex":        ".*.java",
            "name":         "Run as java",
            "command":      "java __PATH__",
            "type":         "java"
        }
    ]
}